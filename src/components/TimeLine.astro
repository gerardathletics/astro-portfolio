---
import { getI18N } from '../i18n/index.ts';
import ExperienceSection from './Experience.tsx';

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });

const experiences = i18n.SECTION_XP.EXPERIENCES;
const studies = i18n.SECTION_XP.STUDIES;
---

<ExperienceSection client:load dataExperience={experiences} dataStudies={studies} />
