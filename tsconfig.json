{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
        "@/*": ["./src/*"],
        "@/components/*": ["./src/components/*"],
        "@/i18n/*": ["./src/i18n/*"]
      },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "node",
  }
}